{
  "name": "tistory-editor",
  "productName": "TistoryEditor",
  "version": "0.3.8",
  "description": "Editor for tistory",
  "main": "src/main/main.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress",
    "watch": "NODE_ENV=development webpack --watch --progress",
    "app": "NODE_ENV=development electron .",
    "start": "concurrently \"yarn run watch\" \"yarn run app\"",
    "test": "jest -u",
    "dist": "build",
    "dist-all": "build --linux --win --mac zip --x64",
    "dist-mac": "build -m --x64",
    "dist-linux": "build -l --x64",
    "dist-win": "build -w --x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joostory/tistory-editor.git"
  },
  "keywords": [
    "editor",
    "tistory"
  ],
  "author": {
    "name": "Joo",
    "email": "joo@joostory.net",
    "url": "http://joostory.net"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joostory/tistory-editor/issues"
  },
  "homepage": "https://github.com/joostory/tistory-editor#readme",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.4",
    "electron": "^1.6.11",
    "electron-builder": "^19.4.2",
    "jest": "^20.0.4",
    "object-assign": "^4.1.1",
    "react-test-renderer": "^15.4.2",
    "style-loader": "^0.18.1",
    "webpack": "^3.3.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.1",
    "classnames": "^2.2.5",
    "dateformat": "^2.0.0",
    "debug": "^2.6.8",
    "delta-to-html": "^0.1.4",
    "electron-json-storage": "^3.0.1",
    "electron-localshortcut": "^2.0.0",
    "electron-oauth2": "^3.0.0",
    "form-data": "^2.1.2",
    "highlightjs": "^9.8.0",
    "isomorphic-fetch": "^2.2.1",
    "marked": "^0.3.6",
    "material-ui": "^0.18.1",
    "material-ui-chip-input": "^0.15.0",
    "node-fetch": "^1.7.1",
    "prop-types": "^15.5.10",
    "querystring": "^0.2.0",
    "quill": "^1.2.5",
    "react": "^15.4.2",
    "react-codemirror": "^1.0.0",
    "react-dom": "^15.4.2",
    "react-dropzone": "^3.13.2",
    "react-redux": "^5.0.2",
    "react-tap-event-plugin": "^2.0.1",
    "react-tinymce": "^0.5.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "tinymce": "^4.6.3",
    "tinymce-plugin-codeblock": "^1.1.5",
    "tinymce-plugin-opengraph": "^0.1.7",
    "to-markdown": "^3.0.3"
  },
  "jest": {
    "testEnvironment": "node",
    "globals": {
      "document": true,
      "window": true,
      "navigator": {
        "userAgent": "Mozilla/5.0 (Macintosh; Intel Mac OS X 10_11_2) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/47.0.2526.106 Safari/537.36"
      }
    }
  },
  "build": {
    "appId": "net.joostory.tistory-editor"
  }
